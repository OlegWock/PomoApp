{
    "name": "PomoApp",
    "description": "Tiny pomodoro app",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icons/icon-128.png"
    },
    "icons": { 
        "16": "icons/icon-16.png", 
        "128": "icons/icon-128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ]
} 
